{
  "name": "operator",
  "version": "0.0.1",
  "description": "Serverless Intercom analysis",
  "repository": "jamesramsay/operator",
  "author": "James Ramsay",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "4.3.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "v": "node -p 'require(\"./package.json\").version'",
    "publish": "npm run pkg:put",
    "pkg:zip": "rm -f lambda.zip && zip -qyr lambda.zip . -x@.lambdaignore",
    "pkg:put": "npm run pkg:zip && aws s3api put-object --bucket intercom-operator --key fn/operator-build-$(npm run -s v).zip --body lambda.zip --acl public-read"
  },
  "dependencies": {
    "aws-sdk": "^2.9.0",
    "bluebird": "^3.4.7",
    "intercom-client": "^2.8.6",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "node-lambda": "^0.8.11"
  }
}
